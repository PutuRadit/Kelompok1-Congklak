def c_to_f( celsius ):
 fahrenheit= (celsius*9/5)+32 
 return fahrenheit
def f_to_c(fahrenheit ):
  celsius= (fahrenheit -32) *5/9
  return celsius

def tampilkan_hasil ():
 c=30
 f=100
 print ("Konversi:",c,"C -> ", c_to_f(c) , "F") 
 print ("Konversi:",f,"F -> ", f_to_c(f), "C")

tampilkan_hasil()
